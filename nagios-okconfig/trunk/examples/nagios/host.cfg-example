

define host {
	use		default-host



	host_name	HOSTNAME
	address		IPADDR

        contact_groups  +office-admins,office-prod-admins
        hostgroups      +office-prod-servers,office-servers
};


define service{
        use                             generic-service
        host_name                       HOSTNAME
        service_description             ping
        check_command                   check_ping!100.0,20%!500.0,60%
}




