#!/bin/sh

DOMAIN="DOMAIN"
DOMAIN_USER="Administrator"
DOMAIN_PASSWORD="PASSWORD"
BATCHFILE='\\nagios\nsclient\install.bat'


echo "Starting install of $1" | tee /var/log/install_nsclient.log
winexe  -U DOMAIN/$DOMAIN_USER%$DOMAIN_PASSWORD //$1 "cmd /c $BATCHFILE"
RESULT=$?

if [ $RESULT -gt 0 ]; then
	echo install of $1 failed | tee /var/log/install_nsclient.log
	exit $RESULT
fi

echo "Starting install of $1 sucessful" | tee /var/log/install_nsclient.log
exit 0

