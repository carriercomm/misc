#!/bin/sh

ln -s /opt/opensource.is/nagios-okconfig/trunk/okconfig /etc/nagios/
ln -s /opt/opensource.is/nagios-plugins/crit2warn.sh/trunk/crit2warn.sh /usr/lib64/nagios/plugins/
