ln -s /opt/opensource.is/nagios-plugins/misc/check_ports /usr/lib64/nagios/plugins/
ln -s /opt/opensource.is/nagios-okconfig/trunk/bin/addgroup /usr/local/bin/
cp /opt/opensource.is/nagios-okconfig/trunk/etc/bash_completion.d/okconfig /etc/bash_completion.d/

